
        <!-- Content -->
<%
meta title: @gym.name,
     description: @gym.description,
     keywords: %w(gymexplore gyms),
     robots: "nofollow"
%>
        <div id="wrapper">
            <div id="mapView" class="mob-min"><div class="mapPlaceholder"><span class="fa fa-spin fa-spinner"></span> Loading map...</div></div>
            <div id="content" class="mob-max">
                <div class="rightContainer">
                    <h1>Update Gym</h1>
                    <%= form_for @gym do |f| %>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="form-group">
                                    <label>Name</label>
                                    <input type="text" class="form-control" name="name" value="<%=@gym.name%>">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="form-group">
                                    <label>Email</label>
                                    <input type="email" class="form-control" name="email" value="<%=@gym.email%>">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="form-group">
                                    <label>Mobile</label>
                                    <input type="text" class="form-control" name="mobile" placeholder="+919632xxxxxx" value="<%=@gym.mobile%>">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="form-group">
                                    <label>Website</label>
                                    <input type="url" class="form-control" name="website" value="<%=@gym.website%>">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Description</label>
                            <textarea class="form-control" rows="4" name="description"><%=@gym.description%></textarea>
                        </div>
                        <div class="form-group">
                            <label>Address <input type="hidden" id="latitude" name="latitude" value="<%=@gym.lang%>" /> <input type="hidden" id="longitude" name="longitude" value="<%=@gym.long%>" /></label>
                            <input class="form-control" type="text" id="address" placeholder="Start Typing Any Famous Landmark Near By" autocomplete="off">
                            <p class="help-block">You can drag the black marker to gym position</p>
                        </div>
                        <div class="form-group">
                            <label>Full Address</label>
                            <textarea class="form-control" rows="4" name="address"><%=@gym.address%></textarea>
                        </div>
                        <div class="form-group">
                            <label>Gymmers</label>
                            <select class="form-control" name="gender">
                                <% if @gym.gender == 0 %>
                                    <option value="1">Male</option>
                                    <option value="2">Female</option>
                                    <option value="0" selected="selected">Male and Female</option>
                                <% elsif @gym.gender == 1 %>
                                    <option value="1" selected="selected">Male</option>
                                    <option value="2">Female</option>
                                    <option value="0">Male and Female</option>
                                <% else %>
                                    <option value="1">Male</option>
                                    <option value="2" selected="selected">Female</option>
                                    <option value="0">Male and Female</option>
                                <% end %>
                            </select>
                        </div>
                        <div class="form-group">
                            <label>Registration Fees</label>
                            <div class="input-group">
                                <div class="input-group-addon">Rs.</div>
                                <input class="form-control" type="text" placeholder="Ex: 500" name="fees" value="<%=@gym.registration_fee%>">
                            </div>
                        </div>
                        <div class="pricing-wrapper">
                            <% @gym.pricings.where(:pricing_type => "regular").each_with_index do |p,i| %>
                                <% if i==0 %>
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                            <div class="form-group">
                                                <label>Duration</label>
                                                <div class="input-group">
                                                    <input class="form-control" type="number" placeholder="Ex: 3" name="duration[]" value="<%=p.duration%>">
                                                    <div class="input-group-addon">Month(s)</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                                            <div class="form-group">
                                                <label>Price</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">Rs.</div>
                                                    <input class="form-control" type="text" placeholder="Ex: 5000.00" name="price[]" value="<%=p.price%>">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <% else %>
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <input class="form-control" type="number" placeholder="Ex: 3" name="duration[]" value="<%=p.duration%>">
                                                    <div class="input-group-addon">Month(s)</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Rs.</div>
                                                    <input class="form-control" type="text" placeholder="Ex: 5000.00" name="price[]" value="<%=p.price%>">
                                                </div>
                                            </div>
                                        </div>
                                        <a href="#" class="btn btn-danger remove-pricing"> - </a>
                                    </div>
                                <% end %>
                            <% end %>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3"><button id="add-pricing" class="btn btn-green btn-xs margin-b-10">Add New Pricing</button></div>
                        </div>
                        <div class="pass-wrapper">
                            <% @gym.pricings.where(:pricing_type => "pass").each_with_index do |p,i| %>
                                <% if i==0 %>
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                            <div class="form-group">
                                                <label>Pass Duration</label>
                                                <div class="input-group">
                                                    <input class="form-control" type="number" placeholder="Ex: 3" name="pass_duration[]" value="<%=p.duration%>">
                                                    <div class="input-group-addon">Day(s)</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                                            <div class="form-group">
                                                <label>Pass Price</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">Rs.</div>
                                                    <input class="form-control" type="text" placeholder="Ex: 5000.00" name="pass_price[]" value="<%=p.price%>">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <% else %>
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <input class="form-control" type="number" placeholder="Ex: 3" name="pass_duration[]" value="<%=p.duration%>">
                                                    <div class="input-group-addon">Day(s)</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Rs.</div>
                                                    <input class="form-control" type="text" placeholder="Ex: 5000.00" name="pass_price[]" value="<%=p.price%>">
                                                </div>
                                            </div>
                                        </div>
                                        <a href="#" class="btn btn-danger remove-pass"> - </a>
                                    </div>
                                <% end %>
                            <% end %>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3"><button id="add-pass" class="btn btn-green btn-xs margin-b-10">Add New Pass</button></div>
                        </div>
                        <div class="row" id="timings-wdm">
