                                    <div class="input-group">
                                        <div class="input-group-addon">To</div>
                                        <input class="form-control timepicker time end" type="text" value="<%=@gym.weekend_to%>" name="weekend_to">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" id="timings-wee">
                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label>Weekend(s) Evening Schedule</label>
                                    <div class="input-group">
                                        <div class="input-group-addon">From</div>
                                        <input class="form-control timepicker time start" type="text" value="<%=@gym.weekend_secondary_from%>" name="weekend_secondary_from">
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <div class="input-group">
                                        <div class="input-group-addon">To</div>
                                        <input class="form-control timepicker time end" type="text" value="<%=@gym.weekend_secondary_to%>" name="weekend_secondary_to">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="form-group">
                                    <label>Image Gallery</label>
                                    <%= f.file_field :images, :class => "file", :multiple => true, :data => { :'show-upload' => "false", :'show-caption' => "false", :'show-remove' => "false", :'browse-class' => "btn btn-o btn-default", :'browse-label' => "Browse Images" }, :accept => "image/jpeg,image/png" %>
                                    <!-- <input type="file" class="file" multiple data-show-upload="false" data-show-caption="false" data-show-remove="false" accept="image/jpeg,image/png" data-browse-class="btn btn-o btn-default" data-browse-label="Browse Images" name="images[]"> -->
                                    <p class="help-block">You can select multiple images at once</p>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label>Number of Trainers</label>
                                    <div class="input-group">
                                        <div class="input-group-addon">Male</div>
                                        <input class="form-control" type="number" placeholder="Ex: 4" name="male_trainers" value="<%=@gym.male_trainers%>">
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <div class="input-group">
                                        <div class="input-group-addon">Female</div>
                                        <input class="form-control" type="number" placeholder="Ex: 2" name="female_trainers" value="<%=@gym.female_trainers%>">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% all_facilities = @gym.facility.split(",") rescue [] %>
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                <div class="form-group">
                                    <label>Facilities</label>
                                    <% if all_facilities.include?("Personal Training") %>
